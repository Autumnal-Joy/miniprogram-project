.tabbar {
  position: fixed;
  left: 0;
  bottom: 0;
  height: 46px;
  width: 100%;
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: space-around;
  border-top: 1px solid #d8d8d8;
}
.tabbar .image {
  width: 100%;
  display: flex;
  justify-content: center;
  align-items: center;
}
.tabbar .image image {
  width: 28px;
}
